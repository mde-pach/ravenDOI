{
  "name": "__MSG_appName__",
  "author": "Raven Project",
  "version": "2.0",
  "description": "__MSG_appDescription__",
  "manifest_version": 2,
  "icons": {
    "16": "icons/raven_icon_16.png",
    "48": "icons/raven_icon_48.png",
    "64": "icons/raven_icon_64.png",
    "128": "icons/raven_icon_128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "19": "icons/raven_icon_19.png",
      "38": "icons/raven_icon_38.png"
    },
    "default_title": "Do I Access",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "page": "options/options.html"
  }
}
